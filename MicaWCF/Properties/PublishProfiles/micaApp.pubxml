<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://micaapp.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>micaapp.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>micaApp</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$micaApp</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="micadb2Entities" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:lioruut4zy.database.windows.net,1433;Initial Catalog=mica_db;User ID=mica@lioruut4zy;Password=Abcd1234" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="MicaWCF.micadb2Entities, MicaWCF" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)micadb2Entities-Web.config Connection String">
      <ParameterValue>metadata=res://*/dbEDMX.csdl|res://*/dbEDMX.ssdl|res://*/dbEDMX.msl;provider=System.Data.SqlClient;provider connection string="Data Source=tcp:lioruut4zy.database.windows.net,1433;Initial Catalog=mica_db;User ID=mica@lioruut4zy;Password=Abcd1234"</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>